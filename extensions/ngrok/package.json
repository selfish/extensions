{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ngrok",
  "title": "Ngrok",
  "description": "Control Ngrok locally",
  "icon": "ngrok.ico",
  "author": "Selfish",
  "license": "MIT",
  "commands": [
    {
      "name": "tunnels",
      "title": "Open Tunnels",
      "subtitle": "ngrok",
      "description": "List all open tunnels",
